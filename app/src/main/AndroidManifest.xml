<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.semesterproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="semesterproject.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="semesterproject.ExerciseStartActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_start_name">
        

        </activity>
        
        <activity android:name="semesterproject.SettingsActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_settings_name"
            android:parentActivityName="semesterproject.MainActivity">

            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="semesterproject.MainActivity"/>

        </activity>



        <activity android:name="semesterproject.CalenderView"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_calendar_name"
            android:parentActivityName="semesterproject.MainActivity">

            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="semesterproject.MainActivity"/>

        </activity>


        <activity android:name="semesterproject.ChangeRestTimeActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/rest_time_text"
            android:parentActivityName="semesterproject.SettingsActivity">

            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="semesterproject.SettingsActivity"/>

        </activity>





        <activity android:name="semesterproject.DifficultyActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_difficulty_name"
            android:parentActivityName="semesterproject.SettingsActivity">

            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="semesterproject.SettingsActivity"/>

        </activity>



        <activity android:name="semesterproject.FinishExerciseActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_finish_name"/>






    </application>

</manifest>